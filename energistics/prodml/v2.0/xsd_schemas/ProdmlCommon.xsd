						<xs:annotation>
							<xs:documentation>The fluid specific gravity.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="StartingCarbonNumber" type="eml:NonNegativeLong" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The starting / smalestl carbon number.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EndingCarbonNumber" type="eml:NonNegativeLong" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The ending / largest carbon number.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AvgMolecularWeight" type="eml:MolecularWeightMeasure" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Average molecular weight.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AvgDensity" type="eml:MassPerVolumeMeasure" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The average fluid density.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="StartingBoilingPoint" type="eml:ThermodynamicTemperatureMeasure" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The starting boiling point measure.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EndingBoilingPoint" type="eml:ThermodynamicTemperatureMeasure" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The ending boiling point measure.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AvgBoilingPoint" type="eml:ThermodynamicTemperatureMeasure" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The average boiling point measure.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Remark" type="eml:String2000" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Remarks and comments about this data item.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PureFluidComponent">
		<xs:annotation>
			<xs:documentation>Pure fluid component. </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="prodml:AbstractFluidComponent">
				<xs:sequence minOccurs="0" maxOccurs="1">
					<xs:element name="Kind" type="prodml:PureComponentEnumExt" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The type of component.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MolecularWeight" type="eml:MolecularWeightMeasure" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The molecular weight of the pure component.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HydrocarbonFlag" type="xs:boolean" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Yes/no  flag indicates if hydrocarbon or not. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Remark" type="eml:String2000" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Remarks and comments about this data item.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="StartEndDate">
		<xs:annotation>
			<xs:documentation>The start and end date for a reporting period.</xs:documentation>
