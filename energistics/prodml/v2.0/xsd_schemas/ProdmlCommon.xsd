			<xs:documentation>Pseudo fluid component.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="prodml:AbstractFluidComponent">
				<xs:sequence minOccurs="0" maxOccurs="1">
					<xs:element name="Kind" type="prodml:PseudoComponentEnumExt" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The type from pseudo component enumeration. </xs:documentation>
						</xs:annotation>
					</xs:element>
