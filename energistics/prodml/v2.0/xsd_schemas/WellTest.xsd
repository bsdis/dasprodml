				<xs:annotation>
					<xs:documentation>initial</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="other">
				<xs:annotation>
					<xs:documentation>other</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="periodic">
				<xs:annotation>
					<xs:documentation>periodic</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
